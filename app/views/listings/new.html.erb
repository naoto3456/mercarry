<%= form_for @listing do |f| %>
	Item's picture : <%= f.file_field :photo %><br>
	<%= f.text_field :name, class:"form-control", placeholder:"NAME" %><br>
	<%= f.text_field :description, class:"form-control", placeholder:"description" %><br>
	<%= f.text_field :categoryl1_id, class:"form-control", placeholder:"category_id : Please input integer value 1-3 (>_<) time over " %><br>
	<%= f.text_field :categoryl2_id, class:"form-control", placeholder:"subcategory_id : Please input integer value 1-3 (>_<) time over " %><br>
	<%= f.text_field :shipping_paid_by, class:"form-control", placeholder:"shipping_paid_by : Please input integer value 1-3 (>_<) time over " %><br>
	<%= f.text_field :price, class:"form-control", placeholder:"price" %><br>
	<%= f.submit  class: 'btn btn-primary' %><br>
<% end %>
